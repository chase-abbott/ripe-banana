{
  "name": "ripe-banana",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --verbose --runInBand",
    "test:watch": "npm run test -- --watch",
    "start": "node -r dotenv/config server.js",
    "start:watch": "nodemon -r dotenv/config server.js",
    "setup-heroku": "npm run create-heroku-db && npm run create-heroku-db -- --as DEV_DATABASE && heroku config:set PGSSLMODE=require && npm run get-dev-db-url",
    "create-heroku-db": "heroku addons:create heroku-postgresql:hobby-dev",
    "get-dev-db-url": "heroku config:get DEV_DATABASE_URL",
    "get-prod-db-url": "heroku config:get DATABASE_URL",
    "setup-db": "npm run data/setup-db.js",
    "setup-db:prod": "heroku run npm run setup-db"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@types/jest": "^26.0.23",
    "@types/pg": "^8.6.0",
    "eslint": "^7.29.0",
    "jest": "^27.0.5",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.2"
  }
}